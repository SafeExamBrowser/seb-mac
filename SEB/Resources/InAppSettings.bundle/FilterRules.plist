			</dict>
			<key>AddSpecifier</key>
			<dict>
				<key>Type</key>
				<string>PSChildPaneSpecifier</string>
				<key>File</key>
				<string>FilterRule</string>
				<key>Title</key>
				<string>Add Filter Rule ...</string>
				<key>Key</key>
				<string>addFilterRuleKey</string>
			</dict>
			<key>Deletable</key>
			<true/>
		</dict>
	</array>
